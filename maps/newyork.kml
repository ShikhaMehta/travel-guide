<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Custom Pins</name>

    <!-- Define a style for your custom Central ParQk icon -->
    <Style id="customIconStyle">
      <IconStyle>
        <Icon>
          <href>https://raw.githubusercontent.com/ShikhaMehta/travel-guide/main/assets/CentralPark.png</href>
        </Icon>
        <scale>1.5</scale> <!-- Adjust size -->
      </IconStyle>
    </Style>

    <!-- Define a red pin style -->
   <!-- <Style id="redPinStyle">
