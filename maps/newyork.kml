<Style id="redPinStyle">
  <IconStyle>
    <color>ff0000ff</color>
    <scale>2</scale>
    <Icon>
      <href>http://maps.google.com/mapfiles/kml/paddle/red-circle.png</href>
    </Icon>
  </IconStyle>
  <LabelStyle>
    <color>ff00ff00</color> <!-- Green color -->
    <scale>2</scale> <!-- Adjust size -->
  </LabelStyle>
  <BalloonStyle>
    <text><![CDATA[
      <div style="font-size:22px; font-weight:bold; color:green;">
        $[name]
      </div>
    ]]></text>
  </BalloonStyle>
</Style>




    <!-- Central Park with Custom Icon -->
    <Placemark>
      <name>Central Park</name>
      <styleUrl>#redPinStyle</styleUrl> <!-- Apply the custom icon style -->
      <Point>
        <coordinates>-73.968285,40.785091,0</coordinates>
      </Point>
    </Placemark>

    <!-- Franklin D. Roosevelt Four Freedoms Park with Red Pin -->
    <Placemark>
      <name>Franklin D. Roosevelt Four Freedoms Park</name>
      <styleUrl>#redPinStyle</styleUrl> <!-- Apply the red pin style -->
      <Point>
        <coordinates>-73.9535,40.7587,0</coordinates>
      </Point>
    </Placemark>
